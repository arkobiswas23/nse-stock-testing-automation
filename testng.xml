<?xml version="1.0" encoding="UTF-8"?>
<suite name="NSE Stock Testing Suite" parallel="tests" thread-count="3">
    <test name="Chrome Browser Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.nse.testing.tests.StockInformationTest"/>
        </classes>
    </test>
    
    <test name="Firefox Browser Test">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.nse.testing.tests.StockInformationTest"/>
        </classes>
    </test>
    
    <test name="Edge Browser Test">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.nse.testing.tests.StockInformationTest"/>
        </classes>
    </test>
</suite>